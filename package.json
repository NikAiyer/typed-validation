{
  "name": "workpop-base",
  "version": "1.0.0",
  "description": "A base NPM Module for writing Workpop Modules",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "babel ./src --ignore test --out-dir ./dist",
    "lint": "eslint ./src",
    "fix": "eslint ./src --fix",
    "testonly": "mocha test/index.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js"
  },
  "devDependencies": {
    "@workpop/eslint-config-workpop": "0.0.5",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^4.1.8",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "eslint": "^3.3.1",
    "eslint-plugin-babel": "^2.2.0",
    "mocha": "^2.5.3",
    "pre-commit": "^1.1.3"
  },
  "dependencies": {
    "babel-runtime": "6.x.x"
  },
  "pre-commit": [
    "lint"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "@workpop/eslint-config-workpop",
      "plugin:import/errors"
    ]
  },
  "author": "Workpop",
  "license": "ISC"
}
